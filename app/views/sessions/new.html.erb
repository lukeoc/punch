<div class="splashPageBanner row">
  <div class="splashStack col s12 valign-wrapper" >
    <h3 class="splashTitle center-align">punch</h3>
  </div>
  <div class="container">
    <div class="container col s12 center-align">
        <%= form_tag '/sessions',  method: :post do  %>
          <div>
            <%= text_field_tag :email, nil, placeholder: "Email" %>
          </div>
          <div>
            <%= password_field_tag :password, nil, placeholder: "Password" %>
          </div>
          
          <input type="submit" value="login" class="waves-effect waves-light btn-large">
        <% end %>
    </div>
  </div>
</div>
